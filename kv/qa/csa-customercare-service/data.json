custcare:
  service:
    consul:
      health:
        message: The application was connected to Consul successfully
  cors:
    allowed:
      origins:
        - 'http://csa-qa-customercare-react-app.qa.aws.idt.net'
        - 'http://localhost:3000'
  jwt:
    expiration:
      period:
        minutes: 60
    cache:
      timeout:
        minutes: 120
  sso:
    http:
      timeout:
        seconds: 60
    login:
      url: 'https://qa1.web.idt.net/mis/sso'
    param:
      return: redirectTo
    app:
      id: 27242
  psf:
    soap:
      endpoint: 'http://psfoneapp.awsqa.idt.net/PSF.svc'
